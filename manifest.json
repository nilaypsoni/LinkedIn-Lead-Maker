{
	"name": "LinkedIn Lead Maker",
	"version": "1.0",
	"description": "LinkedIn Lead Maker Desc",
	"permissions": ["cookies","alarms","webRequest","activeTab","tabs","*://*.linkedin.com/*","*://*.leadmaker.io/*","storage"],
	"browser_action": {
		"default_icon": {
			"128": "images/default_icon_128.png",
			"16": "images/default_icon_16.png",
			"19": "images/default_icon.png",
			"24": "images/default_icon_24.png",
			"32": "images/default_icon_32.png",
			"48": "images/default_icon_48.png"
		},
		"default_popup": "html/popup.html",
		"default_title": "LinkedIn Lead Maker Title"		
	},
	"icons": {
		"128": "images/default_icon_128.png",
		"16": "images/default_icon_16.png",
		"32": "images/default_icon_32.png",
		"48": "images/default_icon_48.png"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"background": {
		"scripts": ["libs/jquery-3.1.1.min.js","js/consts.js","js/background.js"]
	},
	"content_scripts": [
		{"matches": [ "*://*.linkedin.com/*" ],
			"js":["libs/jquery-3.1.1.min.js","js/messages.js","js/consts.js","contentscript/http-request.js","contentscript/companyParser.js","contentscript/content.js"],
			"css":[],
			"run_at":"document_end"
		}
	],
	"web_accessible_resources": [ "images/*","js/*","contentscript/*" ],
	"manifest_version": 2
}
